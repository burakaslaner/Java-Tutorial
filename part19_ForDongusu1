import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int toplam = 0;
        int sayi;
        for(int i = 0; i < 10; i++){
            System.out.println((i+1) +".Sayi Giriniz: ");
            sayi = input.nextInt();
            toplam += sayi;
        }
        System.out.println("Toplam: " +toplam);
    }
}
